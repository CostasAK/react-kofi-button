{
  "name": "react-kofi-button",
  "version": "0.4.4",
  "description": "React Components to link to your Ko-fi profile",
  "author": "CostasAK (https://github.com/CostasAK)",
  "keywords": [
    "ko-fi",
    "react-component",
    "react",
    "button"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CostasAK/react-kofi-button"
  },
  "bugs": {
    "url": "https://github.com/CostasAK/react-kofi-button/issues"
  },
  "main": "dist/index.js",
  "private": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf dist/*",
    "dist": "npm run clean && babel src/components/ --out-dir dist --copy-files --no-copy-ignored --ignore src/**/*.stories.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "sass": "^1.49.9"
  }
}
